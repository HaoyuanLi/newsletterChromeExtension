{
  "manifest_version": 2,

  "name": "food safety check",
  "description": "This extension fetches information from Vancouver Coastal Health",
  "version": "1.0",


  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "https://inspections.vcha.ca/FoodPremises/Table/",
    "tabs",
    "https://maps.googleapis.com/"
  ]
}
